<%# <%= render 'forms/vehicles_details'%>
<h2> </h2>
	<br> <br>
	<h2>Welcome <%= current_user.name %> <%= link_to "Park Vehicle", new_after_user_signin_path(@vehicle),class: "btn btn-sm btn-info text-light"%></h2>
	<table class="table table-striped table-hover">
			<tr class="header">
					<th>Model Name</th>
					<th>Cars</th>
					<th>Action</th>
			</tr>
			
			<% current_user.vehicles.each do |v| %>
				<tr>
					<td><%= v.number%></td>
					<td><%= v.car_model_name%></td>
					<td>
						<%= link_to "See Details", after_user_signin_path(v.id), class: "btn btn-sm btn-success text-light" %>
						<%= link_to "Update Car Details", edit_after_user_signin_path(v.id), class: "btn btn-sm btn-primary text-light" %>
						<%= link_to "Park Out", after_user_signin_path(v.id), class: "btn btn-sm btn-danger text-light", data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"}%>
					</td>
				</tr>
			<% end %>
</table>